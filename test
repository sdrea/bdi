#!/bin/bash

if [ -z "$1" ]; then
./sim-outorder -config config/1M-baseline.cfg -fastfwd 1000000 -max:inst 1000000 reabmk

elif [ $1 = "0" ]; then
./sim-outorder -config config/1M-baseline.cfg -fastfwd 1000000 -max:inst 1000000 reabmk

elif [ $1 = "1" ]; then
./sim-outorder -config config/1M-compressed.cfg -fastfwd 1000000 -max:inst 1000000 reabmk

else

echo Arg $1 not defined.

fi
